using System;

namespace CountOnes
{
    class Program
    {
        static void Main(string[] args)
        {
            string str = "11441155";
            char[] strArray = str.ToCharArray();
            int count = 0;
            foreach(char s in strArray)
            {
                if(s == "1")
                {
                    count++;
                }
               
            }
            
        }
    }
}
